    "react-router-dom": "^6.2.1",
    "react-scroll": "^1.8.7",
    "rehype-parse": "^8.0.4",
    "rehype-raw": "^6.1.1",
    "rehype-remark": "^9.1.2",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
