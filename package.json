    "xstate": "^4.23.1",
    "yup": "^0.31.1"
  },
  "resolutions": {
    "**/@emotion/styled": "^11.0.0"
  },
  "overrides": {
    "glob-parent": "^5.1.2",
    "got": "11.8.5",
    "nth-check": "^2.0.1",
    "chokidar": "^3.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/import-types-preset": "^2.1.12",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.12",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@sentry/vite-plugin": "^2.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/qrcode": "^1.4.2",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-calendar": "^3.9.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-mentions": "^4.1.8",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^8.3.1",
    "@vitest/browser": "^0.34.2",
    "@vitest/coverage-istanbul": "^0.34.3",
    "coveralls": "^3.1.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.4",
    "husky": "^4.3.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "sass": "^1.52.1",
    "source-map-explorer": "^2.5.3",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2",
    "webdriverio": "^8.15.4",
    "webpack": "^5.88.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintIgnore": [
    "src/generated/graphql.tsx",
    "**/node_modules/**",
    "**/build**"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/graphql",
      "<rootDir>/src/generated",
      "<rootDir>/src/sentry"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs",
          "@babel/plugin-transform-react-jsx"
        ],
        "presets": [
          "@babel/preset-typescript"
        ]
      }
    }
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  }
}
