    "react-cookie": "^4.1.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^18.3.1",
    "react-i18next": "^11.15.1",
    "react-image-crop": "^9.0.5",
    "react-image-file-resizer": "^0.4.7",
    "react-intersection-observer": "^9.1.0",
    "react-markdown": "^8.0.3",
