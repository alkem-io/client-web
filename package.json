    "@mui/styles": "^5.2.0",
    "@mui/x-data-grid": "^5.0.1",
    "@mui/x-date-pickers": "^5.0.16",
    "@ory/kratos-client": "^0.10.1",
    "@sentry/react": "^7.69.0",
    "@sentry/tracing": "^7.69.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
