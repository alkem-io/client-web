    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.5.1",
    "sass": "^1.52.1",
    "webpack": "^5.66.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintIgnore": [
    "src/generated/graphql.tsx",
    "**/node_modules/**",
    "**/build**"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/graphql",
      "<rootDir>/src/generated",
      "<rootDir>/src/sentry"
    ]
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  }
