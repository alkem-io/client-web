  "scripts": {
    "prestart": "cross-env NODE_ENV=development gulp buildConfiguration",
    "start": "react-scripts start",
    "prebuild": "cross-env NODE_ENV=production gulp buildConfiguration",
    "build": "react-scripts build",
    "pretest": "NODE_ENV=test gulp buildConfiguration",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --coverage --watchAll=false",
    "test:coverage": "react-scripts test --coverage && cat ./coverage/lcov.info | coveralls",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "gulp": "gulp"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/import-types-preset": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/uuid": "^8.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "gulp": "^4.0.2",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintIgnore": [
    "src/generated/graphql.tsx",
    "**/node_modules/**",
    "**/build**"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/graphql",
      "<rootDir>/src/generated",
      "<rootDir>/src/sentry"
    ]
  },
  "engines": {
    "node": ">=14.17.3",
    "npm": ">=7"
  }
}
