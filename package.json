{
  "name": "@alkemio/client-web",
  "version": "0.11.0",
  "description": "Alkemio client, enabling users to interact with the Challenge centric platform.",
  "author": "Cherrytwist Foundation",
  "repository": {
    "type": "git",
    "url": "https://github.com/alkem-io/Client.Web.git"
  },
  "license": "EUPL-1.2",
  "dependencies": {
    "@apollo/client": "^3.4.9",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@excalidraw/excalidraw": "^0.9.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.56",
    "@mui/material": "^5.2.0",
    "@mui/styles": "^5.2.0",
    "@mui/x-data-grid": "^5.0.1",
    "@ory/kratos-client": "^0.7.6-alpha.1",
    "@sentry/react": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/yup": "^0.29.11",
    "@uiw/react-md-editor": "^3.4.10",
    "@xstate/graph": "^1.3.0",
    "@xstate/react": "^1.5.1",
    "apollo-upload-client": "^16.0.0",
    "axios": "^0.21.1",
    "bootstrap-icons": "1.1.0",
    "clamp-js": "^0.7.0",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "d3": "^7.0.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "formik": "^2.2.6",
    "graphql": "^15.4.0",
    "i18next": "^20.0.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.21",
    "logform": "^2.2.0",
    "nanoid": "^3.1.30",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-cookie-consent": "^6.2.4",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.11.3",
    "react-jss": "^10.5.0",
    "react-markdown": "^6.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-visibility-sensor": "^5.1.1",
    "remark-gfm": "^1.0.0",
    "replace-special-characters": "^1.2.5",
    "typescript": "~4.1.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0",
    "winston": "^3.3.3",
    "winston-react": "^1.0.0-RC.0",
    "winston-transport": "^4.4.0",
    "xstate": "^4.23.1",
    "yup": "^0.31.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp buildConfiguration && react-scripts start",
    "start:services": "docker-compose -f quickstart-services.yml --env-file .env.docker up --build --force-recreate",
    "build": "cross-env NODE_ENV=production gulp buildConfiguration && react-scripts build",
    "test": "cross-env NODE_ENV=test gulp buildConfiguration && react-scripts test",
    "test:cov": "npm run test -- --coverage --watchAll=false",
    "test:coverage": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "gulp": "gulp",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "resolutions": {
    "**/@emotion/styled": "^11.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/import-types-preset": "^2.1.1",
    "@graphql-codegen/typescript": "^2.1.1",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.1",
    "@graphql-codegen/typescript-operations": "^2.1.1",
    "@graphql-codegen/typescript-react-apollo": "^3.1.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/uuid": "^8.3.1",
    "coveralls": "^3.1.1",
    "eslint-config-prettier": "^8.3.0",
    "gulp": "^4.0.2",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintIgnore": [
    "src/generated/graphql.tsx",
    "**/node_modules/**",
    "**/build**"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/graphql",
      "<rootDir>/src/generated",
      "<rootDir>/src/sentry"
    ]
  },
  "engines": {
    "node": ">=14.17.3",
    "npm": ">=7"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
