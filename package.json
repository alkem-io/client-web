    "@vitest/browser": "^2.0.3",
    "@vitest/coverage-istanbul": "^2.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^4.3.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "serve": "^14.2.4",
    "source-map-explorer": "^2.5.3",
    "vite": "^5.3.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.0.3",
    "webpack": "^5.88.1"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10"
  },
  "volta": {
    "node": "20.15.1"
  },
  "overrides": {
    "glob-parent": "^5.1.2",
    "got": "11.8.5",
    "nth-check": "^2.0.1",
    "chokidar": "^3.5.3"
  }
}
